	journal = {Journal of Ecology},
	author = {Kleyheeg, Erik and Treep, Jelle and Jager, Monique de and Nolet, Bart A. and Soons, Merel B.},
	year = {2017},
	keywords = {dispersal, endozoochory, habitat fragmentation, Anas platyrhynchos, aquatic plants, dabbling ducks, freshwater marshes, landscape configuration, mallard, terrestrial plants},
	pages = {1279--1289},
	file = {Full Text PDF:C\:\\Users\\javia\\Zotero\\storage\\AR6PQML5\\Kleyheeg et al. - 2017 - Seed dispersal distributions resulting from landsc.pdf:application/pdf;Snapshot:C\:\\Users\\javia\\Zotero\\storage\\V9EQND7I\\1365-2745.html:text/html}
}

