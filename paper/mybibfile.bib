	year = {2008},
	pages = {19050--19051},
	file = {Full Text PDF:C\:\\Users\\javia\\Zotero\\storage\\72LC259U\\Nathan - 2008 - An emerging movement ecology paradigm.pdf:application/pdf;Snapshot:C\:\\Users\\javia\\Zotero\\storage\\PC43QZFU\\19050.html:text/html}
