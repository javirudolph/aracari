	author = {Chen, Si-Chong and Tamme, Riin and Thomson, Fiona J. and Moles, Angela T.},
	year = {2019},
	keywords = {Biodiversity, seed dispersal, dispersal distance, dispersal mode, Janzen–Connell hypothesis, latitude, plant height, seed mass},
	pages = {954--961},
